<config>
	<context path="hypersearch.quix"
		method="GET"
		client=".*"
		lang=".*"
		action="hypersearch.psp">
		<filter
			type="porcupine.filters.output.I18n"
			using="org.innoscript.desktop.strings.resources"/>
	</context>
	<context path="hypersearch.js"
		method="GET"
		client=".*"
		lang=".*"
		action="hypersearch.js"
		max-age="3600"/>
</config>